{
  "name": "SW",
  "version": "1.0",
  "description": "Chrome extension to scrape sports betting info",
  "manifest_version": 2,
  "permissions": [
    "declarativeContent","storage"
  ],
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "page_action": {
    "default_popup": "/assets/pages/index.html",
    "default_icon": {
      "128": "/assets/images/logo.png"
    }
  },
  "icons": {
    "128": "/assets/images/logo.png"
  }
}
